{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "src",
	"compilerOptions": {
		"deleteOutDir": true,
		"builder": "swc",
		"typeCheck": true
	},
	"projects": {
		"scrape-card": {
			"type": "library",
			"root": "libs/scrape-card",
			"entryFile": "index",
			"sourceRoot": "libs/scrape-card/src",
			"compilerOptions": {
				"tsConfigPath": "libs/scrape-card/tsconfig.lib.json"
			}
		},
		"fs": {
			"type": "library",
			"root": "libs/fs",
			"entryFile": "index",
			"sourceRoot": "libs/fs/src",
			"compilerOptions": {
				"tsConfigPath": "libs/fs/tsconfig.lib.json"
			}
		}
	}
}
