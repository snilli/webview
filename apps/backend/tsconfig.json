{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noImplicitAny": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@lib/scrape-card": [
        "libs/scrape-card/src"
      ],
      "@lib/scrape-card/*": [
        "libs/scrape-card/src/*"
      ],
      "@lib/fs": [
        "libs/fs/src"
      ],
      "@lib/fs/*": [
        "libs/fs/src/*"
      ]
    }
  }
}