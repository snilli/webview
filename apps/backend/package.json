		"axios": "^1.6.8",
		"cheerio": "1.0.0-rc.12",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"vite": "^5.2.11"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.3.2",
		"@nestjs/schematics": "^10.1.1",
		"@nestjs/testing": "^10.3.8",
